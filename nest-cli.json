{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/producer-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/producer-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/producer-app",
  "projects": {
    "consumer-app": {
      "type": "application",
      "root": "apps/consumer-app",
      "entryFile": "main",
      "sourceRoot": "apps/consumer-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/consumer-app/tsconfig.app.json"
      }
    },
    "consumer-app-azure": {
      "type": "application",
      "root": "apps/consumer-app-azure",
      "entryFile": "main",
      "sourceRoot": "apps/consumer-app-azure/src",
      "compilerOptions": {
        "tsConfigPath": "apps/consumer-app-azure/tsconfig.app.json"
      }
    },
    "producer-app": {
      "type": "application",
      "root": "apps/producer-app",
      "entryFile": "main",
      "sourceRoot": "apps/producer-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/producer-app/tsconfig.app.json"
      }
    },
    "producer-app-azure": {
      "type": "application",
      "root": "apps/producer-app-azure",
      "entryFile": "main",
      "sourceRoot": "apps/producer-app-azure/src",
      "compilerOptions": {
        "tsConfigPath": "apps/producer-app-azure/tsconfig.app.json"
      }
    }
  }
}