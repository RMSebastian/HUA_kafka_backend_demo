{
  "name": "producer-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build-all": "concurrently -c \"cyan.bold,red.bold,green.bold,violet.bold\" \"npm run build-pro\"  \"npm run build-pro-azure\"  \"npm run build-con-azure\"  \"npm run build-con\"",
    "build-kafka": "concurrently -c \"cyan.bold,red.bold\" \"npm run build-pro\"  \"npm run build-con\"",
    "build-azure": "concurrently -c \"cyan.bold,red.bold\"  \"npm run build-pro-azure\"  \"npm run build-con-azure\"  ",
    "build-pro": "nest build producer-app",
    "build-pro-azure": "nest build producer-app-azure",
    "build-con": "nest build consumer-app",
    "build-con-azure": "nest build consumer-app-azure",
    "build": "nest build producer-app",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev-all": "concurrently -c \"cyan.bold,red.bold,green.bold,violet.bold\" \"npm run dev-con\" \"npm run dev-pro\"\"npm run dev-pro-azure\" \"npm run dev-con-azure\"",
    "dev-azure": "concurrently -c \"cyan.bold,red.bold\" \"npm run dev-pro-azure\" \"npm run dev-con-azure\"",
    "dev-kafka": "concurrently -c \"cyan.bold,red.bold\" \"npm run dev-con\" \"npm run dev-pro\"",
    "dev-pro": "nest start --watch producer-app",
    "dev-pro-azure": "nest start --watch producer-app-azure",
    "dev-con": "nest start --watch consumer-app",
    "dev-con-azure": "nest start --watch consumer-app-azure",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/producer-app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/producer-app/test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/service-bus": "^7.9.5",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@niur/nestjs-service-bus": "^1.1.0",
    "axios": "^1.9.0",
    "class-validator": "^0.14.2",
    "concurrently": "^9.1.2",
    "kafkajs": "^2.2.4",
    "newrelic": "latest",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "webpack": "^5.99.8",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
